{
  "expo": {
    "name": "RNproject",
    "slug": "RNproject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ebf3e7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "Matias.joao.RNproject", 
      "buildNumber": "1.0.0" 
    },
    "android": {
      "package": "Matias.joao.RNproject",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ebf3e7"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
